<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MenuInicialActivity">

    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:visibility="visible">

        <ImageButton
            android:id="@+id/btnSocial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:cropToPadding="false"
            android:fontFamily="serif"
            android:onClick="btnSocial"
            android:scrollbars="horizontal|none"
            android:shape="rectangle"
            android:soundEffectsEnabled="false"
            android:src="@drawable/saude_social10"
            android:contentDescription="TODO" />


    </TableRow>


    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:visibility="visible">

        <ImageButton
            android:id="@+id/btnFisica"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:fontFamily="serif"
            android:onClick="btnFisica"
            android:shape="rectangle"
            app:srcCompat="@drawable/saude_fisica10"
            android:contentDescription="TODO" />


    </TableRow>


    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:visibility="visible">

        <ImageButton
            android:id="@+id/btnMental"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:fontFamily="serif"
            android:onClick="btnMental"
            android:shape="rectangle"
            app:srcCompat="@drawable/saude_mental10" />


    </TableRow>
</TableLayout>